    "@types/mocha": "^5.0.0",
    "@types/node": "^8.9.4",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "mocha": "^5.0.5",
    "parcel-bundler": "^1.6.2",
    "tslint": "^5.9.1",
